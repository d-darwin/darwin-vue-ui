{
  "name": "@darwin-studio/ui-vue",
  "description": "Vue 3 based lightweight ui-library",
  "version": "0.12.2",
  "author": "Dmitrii Bykov <d@darwin.st> (https://darwin.st/)",
  "license": "MIT",
  "homepage": "https://github.com/d-darwin/darwin-vue-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-darwin/darwin-vue-ui.git"
  },
  "bugs": {
    "url": "https://github.com/d-darwin/darwin-vue-ui/issues"
  },
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "sideEffects": false,
  "files": [
    "src/assets/*",
    "src/components/*",
    "src/use/*",
    "src/mixins/*",
    "src/utils/*",
    "src/index.js",
    "*.json",
    "*.js"
  ],
  "keywords": [
    "Vue 3",
    "UI library",
    "Visual components",
    "CSS custom properties"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "test": "vue-cli-service test:unit --config ./jest.config.json",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name ui-vue ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "autoprefixer": "^10.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.1.0",
    "postcss": "^8.1.4",
    "prettier": "^1.19.1",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "vue": "^3.0.2"
  },
  "peerDependencies": {
    "vue": "^3.0.2"
  },
  "dependencies": {
    "focus-visible": "^5.2.0"
  }
}
